{
  "name": "realtime-bpm-analyzer",
  "version": "1.0.5",
  "description": "Tool to analyze BPM / Tempo in realtime",
  "author": "David Lepaux <d.lepaux@gmail.com> (https://github.com/dlepaux)",
  "keywords": [
    "webaudioapi",
    "audiobuffer",
    "audio",
    "realtime",
    "tempo",
    "bpm",
    "beats",
    "minutes",
    "analyzer"
  ],
  "main": "./src/index.js",
  "scripts": {
    "debug": "set LOGGY_STACKS=true && brunch w",
    "test": "node_modules/.bin/mocha --require babel-core/register",
    "build": "babel --presets es2015 src --out-dir build",
    "browserify": "browserify build/analyzer.js -o build/bundle.js && browserify build/webaudiotestapi.js -o build/webaudiotestapi-final.js",
    "local-test": "nyc --reporter=html npm test",
    "coverage": "nyc --reporter=text-lcov npm test | coveralls",
    "coveralls": "nyc --reporter=text-lcov npm test && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dlepaux/realtime-bpm-analyzer"
  },
  "dependencies": {
    "babel-plugin-transform-object-assign": "^6.22.0",
    "babel-preset-es2015": "^6.1.2",
    "chai": "^4.1.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babelify": "^8.0.0",
    "browserify": "^16.2.2",
    "coveralls": "^3.0.0",
    "express": "^4.16.3",
    "mocha": "~5.0.0",
    "node-horseman": "^3.3.0",
    "nyc": "^11.2.1",
    "phantomjs": "^2.1.7",
    "web-audio-test-api": "^0.5.2"
  },
  "license": "MIT"
}
